    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "patches": {
            "drupal/editor_file": {
                "3057895 - Uploaded files are not permanent":
                    "https://www.drupal.org/files/issues/2019-05-29/file-upload-marked-permanent-3057895-2.patch"
            },
            "drupal/youtube_playlist": {
                "Mudanças temporárias para uso na FFLCH":
                    "patches/youtube_playlist/2e033f41.patch"
            },
            "drupal/panelbutton": {
                "3105283 - fix location":
                    "patches/panelbutton/1.3.patch"
            },
            "drupal/colorbutton": {
                "3046766 - fix location":
                    "patches/colorbutton/1.2.patch"
            },
            "drupal/anchor_link": {
                "fix location":
                    "patches/anchor_link/2.4.patch"
            },
            "drupal/ckeditor_tableresize": {
                "fix location":
                    "patches/ckeditor_tableresize/1.0.patch"
            },
            "drupal/ckeditor_resize": {
                "fix location":
                    "patches/ckeditor_resize/1.3.patch"
            }
        },
        "installer-types": [
            "npm-asset",
            "bower-asset"
        ],
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/juicebox": [
                "npm-asset/juicebox_lite"
            ],
            "web/libraries/youtube": [
                "npm-asset/ckeditor_youtube"
            ],
            "web/libraries/ckeditor": [
                "bower-asset/ckeditor-dev"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:npm-asset",
                "type:bower-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        }
    }
