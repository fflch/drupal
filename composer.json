{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=5.6",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/console": "^1.0.2",
        "drupal/core": "8.6.9",
        "drupal/crop": "1.5.0",
        "drupal/smtp": "1.0-beta4",
        "drupal/webform": "5.1",
        "drush/drush": "^8.0.0",
        "balbuf/drupal-libraries-installer": "1.0",
        "drupal/single_datetime": "1.6",
        "parsecsv/php-parsecsv": "1.0",
        "drupal/address": "1.0",
        "drupal/block_class": "1.0",
        "drupal/backup_migrate": "4.0",
        "drupal/checklistapi": "1.8",
        "drupal/date_range_formatter": "3.1",
        "drupal/facets": "1.2",
        "drupal/field_group": "1.0",
        "drupal/image_widget_crop": "2.2",
        "drupal/imce": "1.7",
        "drupal/mailsystem": "4.1",
        "drupal/pathauto": "1.0",
        "drupal/metatag": "1.7",
        "drupal/recaptcha": "2.3",
        "drupal/redirect": "1.3",
        "drupal/semanticviews": "2.2",
        "drupal/seo_checklist": "4.1",
        "drupal/simplify_menu": "2.0",
        "drupal/twig_tweak": "2.1",
        "drupal/search_api": "1.7",
        "drupal/token": "1.1",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "^8.6.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
